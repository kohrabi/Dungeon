<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>
	<action label="top">
		<repeat>
			<times>42</times>
			<action>
				<fire>
					<direction type="absolute">360 * $rand</direction>
					<speed>1 + 1 * $rank</speed>
					<bullet />
				</fire>

				<wait>10</wait>
			</action>
		</repeat>
	</action>
</bulletml>